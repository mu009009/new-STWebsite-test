        .social-media-icon-box.background-purple#blog_Icon{ onClick: "boxOnclick('blog.html')"}
        %a.btn.btn-secondary-blue.sub-margin-top{ href: "blog.html"} Blog
      .col-md-4.col-md-offset-0.col-xs-10.col-xs-offset-1.social-media.main-margin-top
        .social-media-icon-box.background-blue#resource_Icon{ onClick: "boxOnclick('http://help.stratifyd.com/support/home')"}
        %a.btn.btn-secondary-blue.sub-margin-top{ href: "http://help.stratifyd.com/support/home"}  Help Center

:javascript
  function boxOnclick(targetPage){
    window.location.href = targetPage;
  }
